    "express-validator": "^3.2.1",
    "history": "^4.6.3",
    "immutable": "^3.8.1",
    "material-ui": "^0.18.7",
    "mdi": "^2.0.46",
    "mongoose": "^4.11.4",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "randomcolor": "^0.5.3",
    "react": "^15.6.1",
    "react-color": "^2.13.4",
    "react-dom": "^15.6.1",
    "react-router": "^4.1.2",
    "react-router-dom": "^4.1.2",
    "react-tap-event-plugin": "^2.0.1",
